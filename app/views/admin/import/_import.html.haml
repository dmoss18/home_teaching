= semantic_form_for :assignments, url: { action: 'assignments' }, html: { multipart: true } do |f|
  = f.inputs do
    = f.input :file, as: :file, label: 'Assignments file (csv format only)'
  = f.actions do
    = f.action :submit

= semantic_form_for :households, url: { action: 'households' }, html: { multipart: true } do |f|
  = f.inputs do
    = f.input :file, as: :file, label: 'Households file (csv format only)'
  = f.actions do
    = f.action :submit
